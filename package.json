{
  "name": "react-obvious",
  "version": "1.0.0",
  "description": "a react libary to manage socket of obvious.js ",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "jest --coverage",
    "lint": "eslint --fix --ext .js,.vue src",
    "demo:install": "cd ./examples/react-app && npm install && cd ../vue-app && npm install && cd ../host-enviroment && npm install",
    "demo:react": "npm run build && cd ./examples/react-app && npm run start",
    "demo:vue": "cd ./examples/vue-app && npm run serve",
    "demo:host": "cd ./examples/host-enviroment && npm start"
  },
  "author": "Philip Lau",
  "license": "MIT",
